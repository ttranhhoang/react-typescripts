	"dependencies": {
		"@craco/craco": "^6.4.5",
		"@testing-library/jest-dom": "^5.16.5",
		"@testing-library/react": "^13.3.0",
		"@testing-library/user-event": "^13.5.0",
		"@types/jest": "^27.5.2",
		"@types/node": "^16.11.56",
		"@types/react": "^18.0.18",
		"@types/react-dom": "^18.0.6",
		"axios": "^0.27.2",
		"mobx": "^6.6.2",
		"mobx-react": "^7.5.3",
		"rc-pagination": "^3.1.17",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
